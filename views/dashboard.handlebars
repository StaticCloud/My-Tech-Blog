<div class="dashboard-wrapper">
    <div class="upper-dashboard">
        <h1>Dashboard</h1>
        <h4>Logged in as <i>{{username}}</i></h4>
        <button type="click"><a href="dashboard/create">Create New Post</a></button>
    </div>
    <ul>
        {{#each posts as |post|}}
        <li>
            {{> post-info post}}
        </li>
        {{/each}}
    </ul>
</div>